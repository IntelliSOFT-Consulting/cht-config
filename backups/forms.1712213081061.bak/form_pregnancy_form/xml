      <bind nodeset="/pregnancy_form/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/pregnancy_form/inputs/source" type="string"/>
      <bind nodeset="/pregnancy_form/inputs/source_id" type="string"/>
      <bind nodeset="/pregnancy_form/inputs/user/contact_id" type="string"/>
      <bind nodeset="/pregnancy_form/inputs/user/parent/_id" type="string"/>
      <bind nodeset="/pregnancy_form/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/pregnancy_form/inputs/contact/name" type="string"/>
      <bind nodeset="/pregnancy_form/inputs/contact/short_name" type="string"/>
      <bind nodeset="/pregnancy_form/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/pregnancy_form/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/pregnancy_form/inputs/contact/sex" type="string"/>
      <bind nodeset="/pregnancy_form/inputs/contact/parent/_id" type="string"/>
      <bind nodeset="/pregnancy_form/inputs/contact/parent/parent/contact/chw_name" type="string"/>
      <bind nodeset="/pregnancy_form/inputs/contact/parent/parent/contact/phone" type="string"/>
      <bind nodeset="/pregnancy_form/patient_age_in_years" type="string" calculate="floor( difference-in-months( ../inputs/contact/date_of_birth, today() ) div 12 )"/>
      <bind nodeset="/pregnancy_form/patient_age_in_months" type="string" calculate="difference-in-months(  /pregnancy_form/inputs/contact/date_of_birth , today() )"/>
      <bind nodeset="/pregnancy_form/patient_age_in_days" type="string" calculate="floor(decimal-date-time(today()) - decimal-date-time( /pregnancy_form/inputs/contact/date_of_birth ) )"/>
      <bind nodeset="/pregnancy_form/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/pregnancy_form/patient_id" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/pregnancy_form/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/pregnancy_form/patient_sex" type="string" calculate="../inputs/contact/sex"/>
      <bind nodeset="/pregnancy_form/visited_contact_uuid" type="string" calculate="../inputs/contact/parent/_id"/>
      <bind nodeset="/pregnancy_form/patient_short_name" type="string" calculate="../inputs/contact/short_name"/>
      <bind nodeset="/pregnancy_form/patient_display_name" type="string" calculate="if(../patient_short_name = '', ../patient_name, concat(../patient_name, ' (', ../patient_short_name, ')'))"/>
      <bind nodeset="/pregnancy_form/reporter/group_report/group_report_adr/medication" type="select1" required="true()"/>
      <bind nodeset="/pregnancy_form/reporter/group_report/group_report_adr/medication_time" type="select1" relevant="selected( /pregnancy_form/reporter/group_report/group_report_adr/medication ,'Yes')" required="true()"/>
      <bind nodeset="/pregnancy_form/reporter/group_report/group_report_adr/immunization" type="select1" required="true()"/>
      <bind nodeset="/pregnancy_form/reporter/group_report/group_report_adr/immunization_time" type="select1" relevant="selected( /pregnancy_form/reporter/group_report/group_report_adr/immunization , 'Yes')" required="true()"/>
      <bind nodeset="/pregnancy_form/reporter/group_report/group_report_adr/reaction" type="select1" relevant="selected( /pregnancy_form/reporter/group_report/group_report_adr/medication ,'Yes') or selected( /pregnancy_form/reporter/group_report/group_report_adr/immunization , 'Yes')" required="true()"/>
      <bind nodeset="/pregnancy_form/reporter/group_report/group_report_quality/medicine" type="select1" required="true()"/>
      <bind nodeset="/pregnancy_form/reporter/group_report/group_report_death/death" type="select1" required="true()"/>
      <bind nodeset="/pregnancy_form/group_review/submit" readonly="true()" type="string"/>
      <bind nodeset="/pregnancy_form/group_review/r_summary_details" readonly="true()" type="string"/>
      <bind nodeset="/pregnancy_form/group_review/c_patient_age" type="string" calculate="if(../../patient_age_in_days &lt; 31, 
concat(../../patient_age_in_days, ' days old'),
if(../../patient_age_in_months &lt; 12, 
concat(../../patient_age_in_months, ' months old'), 
concat(../../patient_age_in_years, ' years old')))"/>
      <bind nodeset="/pregnancy_form/group_review/r_patient_details" readonly="true()" type="string"/>
      <bind nodeset="/pregnancy_form/group_review/r_adr" readonly="true()" type="string"/>
      <bind nodeset="/pregnancy_form/group_review/r_reaction_res" readonly="true()" type="string" relevant="selected( /pregnancy_form/reporter/group_report/group_report_adr/medication ,'Yes') or selected( /pregnancy_form/reporter/group_report/group_report_adr/immunization , 'Yes')"/>
      <bind nodeset="/pregnancy_form/group_review/r_quality" readonly="true()" type="string"/>
      <bind nodeset="/pregnancy_form/group_review/r_medicine" readonly="true()" type="string"/>
      <bind nodeset="/pregnancy_form/group_review/r_medicine_res" readonly="true()" type="string"/>
      <bind nodeset="/pregnancy_form/group_review/r_death_title" readonly="true()" type="string"/>
      <bind nodeset="/pregnancy_form/group_review/r_death" readonly="true()" type="string"/>
      <bind nodeset="/pregnancy_form/group_review/r_death_res" readonly="true()" type="string"/>
      <bind nodeset="/pregnancy_form/group_review/r_letter" readonly="true()" type="string"/>
      <bind nodeset="/pregnancy_form/group_review/r_info" readonly="true()" type="string"/>
      <bind nodeset="/pregnancy_form/data/meta/__patient_uuid" type="string" calculate="../../../inputs/contact/_id"/>
      <bind nodeset="/pregnancy_form/data/meta/__patient_id" type="string" calculate="../../../inputs/contact/patient_id"/>
      <bind nodeset="/pregnancy_form/data/meta/__household_uuid" type="string" calculate="../../../inputs/contact/parent/_id"/>
      <bind nodeset="/pregnancy_form/data/meta/__source" type="string" calculate="../../../inputs/source"/>
      <bind nodeset="/pregnancy_form/data/meta/__source_id" type="string" calculate="../../../inputs/source_id"/>
      <bind nodeset="/pregnancy_form/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
